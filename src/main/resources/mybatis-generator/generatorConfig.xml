<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!-- 数据库驱动包位置 -->
	<classPathEntry
		location="D:\java\mysql-connector-java-5.1.38.jar" />
	<context id="context1">
		<commentGenerator>
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
			<!-- 抑制警告 -->
			<property name="suppressTypeWarnings" value="true" />
			<!-- 是否生成注释代时间戳 -->
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<!-- 数据库链接URL、用户名、密码 --><!-- ?useSSL=false&amp;characterEncoding=utf8&amp;serverTimezone=Asia/Shanghai -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://192.168.18.129:3306/wz-glasses?characterEncoding=utf-8"
			userId="root" password="root" />
		<!-- 生成模型的包名和位置 -->
		<javaModelGenerator targetPackage="com.wz.pojo"
			targetProject="wz-glasses" />
		<!-- 生成的映射文件报名和位置 -->
		<sqlMapGenerator targetPackage="com.wz.mapper"
			targetProject="wz-glasses" />
		<!-- 生成DAO的包名和位置 -->
		<javaClientGenerator targetPackage="com.wz.dao"
			targetProject="wz-glasses" type="XMLMAPPER" />
		<!-- 要生成的那些表(更改tableName 和domainObjectName 就可以了) -->
		<table schema="ssm1" tableName="a_order"
			domainObjectName="Order" mapperName="OrderMapper"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
		</table>
	</context>
</generatorConfiguration>